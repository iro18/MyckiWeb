<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %} Mycki Web{% endblock %}</title>
        {% include('MyckiBundle:Default:stylesheet.html.twig') %}
         <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="{% block body_class %}sub_page{% endblock %}">
        <header id="header" >
        {% include('MyckiBundle:Default:header.html.twig') %}
        </header>
        <div class="clearfix"></div>
        {% block body %}{% endblock %}
        {% include('MyckiBundle:Default:footer.html.twig') %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
